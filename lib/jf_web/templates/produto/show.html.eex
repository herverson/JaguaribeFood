<h2>Ver Produto</h2>

<ul>

  <li>
    <strong>Nome:</strong>
    <%= @produto.nome %>
  </li>

  <li>
    <strong>Descrição:</strong>
    <%= @produto.descricao %>
  </li>

  <li>
    <strong>Preço:</strong>
    <%= @produto.preco %>
  </li>

  <li>
    <strong>Imagem:</strong>
    <%= @produto.imagem %>
  </li>

</ul>

<span><%= link "Editar", to: produto_path(@conn, :edit, @produto) %></span>
<span><%= link "Voltar", to: produto_path(@conn, :index) %></span>
